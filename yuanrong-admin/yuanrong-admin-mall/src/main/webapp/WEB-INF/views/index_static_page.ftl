    //首页轮播图
	var $key = 0;
	var secTime = 3000;
	var timer = setInterval(autoplay, secTime);  /*开启定时器*/
	function autoplay(){
		$("#js-banner-wrap ul li").eq($key).fadeOut(600);  /*第一张图片 淡出 */
		$key ++;
		$key = $key%$("#js-banner-wrap ul li").length;  /*比较好的写法  利用取余的出来的*/
		$("#js-banner-wrap ul li").eq($key).fadeIn(600);
		$("#js-banner-wrap ol li").eq($key).addClass('current').siblings().removeClass('current');
	}
	$("#js-banner-wrap").hover(function() {
		clearInterval(timer);
		timer = null;
	}, function() {
		clearInterval(timer);  
		timer=setInterval(autoplay, secTime);  
	});
	$("#js-banner-wrap ol li").click(function(event) {
		$("#js-banner-wrap ul li").eq($key).fadeOut(600); 
		$key=$(this).index();
		$(this).addClass('current').siblings().removeClass("current");
		$("#js-banner-wrap ul li").eq($key).fadeIn(600);

	});
</script>
